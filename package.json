{
  "name": "ms-chromium-edge-driver",
  "version": "0.2.2",
  "keywords": [
    "edge",
    "edgedriver",
    "msedgedriver",
    "webdriver",
    "selenium"
  ],
  "description": "NPM wrapper for Webdriver Chromium-based Edge driver",
  "main": "lib/index.js",
  "bin": {
    "msedgedriver": "./bin/msedgedriver"
  },
  "typings": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": "git@github.com:dmlemeshko/ms-edge-driver.git",
  "author": "<dima.inspired@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "install": "node lib/install.js",
    "test": "jest",
    "build": "rm -rf lib bin msedgedriver.zip paths.json && tsc -p .",
    "buildWin": "del /s/q lib bin msedgedriver.zip paths.json & tsc -p .",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "dependencies": {
    "extract-zip": "^2.0.0",
    "got": "^10.7.0",
    "lodash": "^4.17.15",
    "regedit": "^3.0.3",
    "util": "^0.12.2"
  },
  "devDependencies": {
    "@types/extract-zip": "^1.6.2",
    "@types/got": "^9.6.9",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.2",
    "jest": "^25.3.0",
    "jest-mock-process": "^1.3.2",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
